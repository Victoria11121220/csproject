# IoT Operator Environment Variables

# PostgreSQL Database URI
POSTGRES_URI=postgresql://postgres:123456@postgres-service.default.svc.cluster.local:5432/iot_dataflow_manager

# Kafka Configuration
KAFKA_BROKERS=kafka-service.default.svc.cluster.local:9092
KAFKA_TOPIC=iot-data

# Container Images
COLLECTOR_IMAGE=iot-collector:dev
PROCESSOR_IMAGE=iot-processor:dev

# Image Pull Configuration
IMAGE_PULL_SECRET=

# Kubernetes Namespace (for testing outside of cluster)
NAMESPACE=default